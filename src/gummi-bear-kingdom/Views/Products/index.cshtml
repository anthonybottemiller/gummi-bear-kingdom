@{
    ViewData["Title"] = "Products";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<GummiBearKingdom.Models.Product>

 <h1>Products</h1>
 <ul>
    @foreach(var product in Model)
    {
        <li>@Html.DisplayFor(productLine => product.Name) | @Html.ActionLink("Details", "Details", new { id = product.ProductId }) | @Html.ActionLink("Delete", "Delete", new { id = product.ProductId })</li>
    }
 </ul>
